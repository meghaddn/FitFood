<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ini.cmu.com.fitfood" >

    <permission android:name="android.permission.GET_ACCOUNTS" >
    </permission>
    <permission android:name="android.permission.INTERNET" >
    </permission>

    <uses-permission android:name="android.permission.GET_ACCOUNTS" />
    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MyActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="com.google.android.gms.version"
            android:value="@integer/google_play_services_version" />

        <activity
            android:name=".SearchFood"
            android:label="@string/title_activity_search_food" >
        </activity>
        <activity
            android:name=".HomePage"
            android:label="@string/title_activity_home_page" >
        </activity>
        <activity
            android:name=".Kitchen"
            android:label="@string/title_activity_kitchen" >
        </activity>
        <activity

            android:name=".Quantity"
            android:label="@string/title_activity_quantity" >
        </activity>
        <activity
            android:name=".NoteEditor"
            android:label="@string/title_activity_note_editor" >
        </activity>
        <activity
            android:name=".EstimateCalorie"
            android:label="@string/title_activity_estimate_calorie" ></activity>

        <activity    android:name=".ProgressCheck"
            android:label="@string/title_activity_progress_check" ></activity>
        <activity    android:name=".ViewReciepeList"
            android:label="@string/title_activity_progress_check" >
        </activity>
    </application>

</manifest>
